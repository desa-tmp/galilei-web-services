  "query": "\n      UPDATE stars\n      SET name = COALESCE($1, name),\n        nebula = COALESCE($2, nebula),\n        public_domain = (CASE WHEN $3 = true THEN $4 ELSE public_domain END)\n      WHERE galaxy_id = $5 AND id = $6\n      RETURNING *\n    ",
