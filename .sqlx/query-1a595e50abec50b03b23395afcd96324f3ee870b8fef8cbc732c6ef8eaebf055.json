  "query": "\n      UPDATE stars\n      SET name = COALESCE($1, name),\n        nebula = COALESCE($2, nebula),\n        public_domain = (CASE WHEN $3 = true THEN $4 ELSE public_domain END),\n        port = COALESCE($5, port)\n      WHERE galaxy_id = $6 AND id = $7\n      RETURNING *\n    ",
